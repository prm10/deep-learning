function [VV,Dim]=dbm_initial(batchdata,num)
dataIn=batchdata;
VV=[];
Dim=[];
vishid=cell(0);
hidbiases=cell(0);
visbiases=cell(0);
for i1=1:length(num)
    [dataIn,vishid{i1},hidbiases{i1},visbiases{i1}]=rbm_model(dataIn,num(i1),10);
end
for i1=1:length(num)
    weight=[vishid{i1};hidbiases{i1}];
    VV=[VV weight(:)'];
end
for i1=length(num):-1:1
    weight=[vishid{i1}';visbiases{i1}];
    VV=[VV weight(:)'];
end
end